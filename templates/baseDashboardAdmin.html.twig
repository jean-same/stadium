<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Stadium -
			{% endblock %}
		</title>

		{% block stylesheets %}
			<link rel="preconnect" href="https://fonts.gstatic.com">
			<link href="https://fonts.googleapis.com/css2?family=Nunito:wght@300;400;600;700;800&display=swap" rel="stylesheet">
			{{ encore_entry_link_tags('app') }}

		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body id="app">
		{% for rubrique, messages in app.flashes %}
			<div class="alert alert-{{ rubrique }} text-center">
				{% for message in messages %}
					<p>
						{{ message }}
					</p>
				{% endfor %}
			</div>
		{% endfor %}
		{%  include "_partials/_sidebarDashboard.html.twig" with {
			'path' : path('dashboards_admin_home'),
			'role' : 'Admin'
		}%}
		{% block body %}{% endblock %}

	</body>
</html>
